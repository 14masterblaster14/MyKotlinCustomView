<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

        <com.example.a5circularprogressbar.CustomProgressBar
            android:id="@+id/custom_circular_progress_bar1"
            android:layout_width="80dp"
            android:layout_height="80dp"
            app:backgroundStrokeWidth="@dimen/backgroundStrokeWidth"
            app:layout_constraintBottom_toTopOf="@id/custom_circular_progress_bar2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:progressStrokeWidth="@dimen/progressStrokeWidth"
            app:spaceAfterTitle="@dimen/spaceAfterTitle"
            app:title="70"
            app:unit="%" />

        <com.example.a5circularprogressbar.CustomProgressBar
            android:id="@+id/custom_circular_progress_bar2"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:indeterminateOnly="false"
            app:backgroundStrokeWidth="@dimen/backgroundStrokeWidth"
            app:layout_constraintBottom_toTopOf="@id/custom_circular_progress_bar3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/custom_circular_progress_bar1"
            app:progressStrokeWidth="@dimen/progressStrokeWidth"
            app:spaceAfterTitle="@dimen/spaceAfterTitle"
            app:title="70"
            app:unit="%" />

        <com.example.a5circularprogressbar.CustomProgressBar
            android:id="@+id/custom_circular_progress_bar3"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:indeterminateOnly="false"
            app:backgroundStrokeWidth="@dimen/backgroundStrokeWidth"
            app:layout_constraintBottom_toTopOf="@id/custom_circular_progress_bar4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/custom_circular_progress_bar2"
            app:progressStrokeWidth="@dimen/progressStrokeWidth"
            app:spaceAfterTitle="@dimen/spaceAfterTitle"
            app:title="70"
            app:unit="%" />

        <com.example.a5circularprogressbar.CustomProgressBar
            android:id="@+id/custom_circular_progress_bar4"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:indeterminateOnly="false"
            app:backgroundStrokeWidth="@dimen/backgroundStrokeWidth"
            app:layout_constraintBottom_toTopOf="@id/custom_circular_progress_bar5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/custom_circular_progress_bar3"
            app:progressStrokeWidth="@dimen/progressStrokeWidth"
            app:spaceAfterTitle="@dimen/spaceAfterTitle"
            app:title="70"
            app:unit="%" />

        <com.example.a5circularprogressbar.CustomProgressBar
            android:id="@+id/custom_circular_progress_bar5"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:indeterminateOnly="false"
            app:backgroundStrokeWidth="@dimen/backgroundStrokeWidth"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/custom_circular_progress_bar4"
            app:progressStrokeWidth="@dimen/progressStrokeWidth"
            app:spaceAfterTitle="@dimen/spaceAfterTitle"
            app:title=""
            app:unit="" />

    </android.support.constraint.ConstraintLayout>

</layout>